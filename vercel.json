{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://toolz.digital https://www.toolz.digital"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://toolz.digital"
        }
      ]
    }
  ]
}